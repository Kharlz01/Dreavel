.container {
  background-color: #222;
  color: #fff;
  padding: 20px;
  width: 100vh;
}

.title {
  font-size: 26px;
  margin-bottom: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.add-button {
  position: fixed;
  bottom: 60px;
  right: 30px;
  background-color: #fff;
  color: #333;
  font-size: 40px;
  border-radius: 50%;
  width: 50px;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}